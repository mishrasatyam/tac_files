{
  "manifest_version": 2,
  "name": "TAC Discovery",
  "description": "This is the extention for TAC Discovery and adding sites to the discovery network. ",
  "version": "1.2",

  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}