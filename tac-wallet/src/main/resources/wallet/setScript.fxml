<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@../mainView/mainView.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox layoutX="42.0" layoutY="14.0" prefWidth="650.0" styleClass="fr" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label styleClass="form" text="%script" />
            <TextArea fx:id="scriptTextArea" prefHeight="350.0" prefWidth="400.0" wrapText="true" VBox.vgrow="ALWAYS" />
            <GridPane maxWidth="1.7976931348623157E308">
               <columnConstraints>
                  <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" />
                  <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Label fx:id="scriptWarningLabel" alignment="CENTER_RIGHT" graphicTextGap="0.0" prefHeight="17.0" prefWidth="57.0" textFill="#7995b9" GridPane.columnIndex="1" />
                  <Label styleClass="form" text="%estimated_fee" />
               </children>
            </GridPane>
            <VBox fillWidth="false" prefWidth="300.0">
               <children>
                  <TextField fx:id="feeTextField" editable="false" prefWidth="300.0" />
                  <Button fx:id="sendButton" disable="true" mnemonicParsing="false" text="%send">
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
         <padding>
            <Insets left="20.0" top="10.0" />
         </padding>
      </VBox>
   </children>
   <padding>
      <Insets bottom="30.0" top="10.0" />
   </padding>
</AnchorPane>
