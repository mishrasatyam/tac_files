                <Tab fx:id="massTransferTab" closable="false" text="%mass_transfer">
                    <fx:include fx:id="massTransferPage" source="massTransfer.fxml" />
                </Tab>
                <Tab fx:id="setScriptTab" closable="false" text="%set_script">
