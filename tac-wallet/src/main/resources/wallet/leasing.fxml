<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="rootPane" minWidth="500.0" stylesheets="@../mainView/mainView.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <VBox minWidth="400.0">
                            <children>
                                <HBox alignment="TOP_CENTER">
                                    <VBox.margin>
                                        <Insets bottom="10.0" />
                                    </VBox.margin>
                                    <children>
                                        <Label styleClass="walletView-label" text="%balance" />
                                    </children>
                                </HBox>
                                <AnchorPane styleClass="leasing-balance-anchor">
                                    <children>
                                        <Label text="%regular" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                        <Label fx:id="regularBalanceLabel" alignment="TOP_RIGHT" contentDisplay="RIGHT" layoutX="222.0" text="0.0 TAC" wrapText="true" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                    </children>
                                    <VBox.margin>
                                        <Insets />
                                    </VBox.margin>
                                </AnchorPane>
                                <AnchorPane styleClass="leasing-balance-anchor">
                                    <children>
                                        <Label text="%effective" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                        <Label fx:id="effectiveBalanceLabel" alignment="TOP_RIGHT" contentDisplay="RIGHT" layoutX="222.0" text="0.0 TAC" wrapText="true" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                    </children>
                                    <opaqueInsets>
                                        <Insets />
                                    </opaqueInsets>
                                </AnchorPane>
                                <AnchorPane styleClass="leasing-balance-anchor">
                                    <children>
                                        <Label text="%available" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                        <Label fx:id="availableBalanceLabel" alignment="TOP_RIGHT" contentDisplay="RIGHT" layoutX="222.0" text="0.0 TAC" wrapText="true" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                    </children>
                                    <opaqueInsets>
                                        <Insets />
                                    </opaqueInsets>
                                </AnchorPane>
                                <AnchorPane styleClass="leasing-balance-anchor">
                                    <children>
                                        <Label prefHeight="17.0" prefWidth="133.0" text="%generating" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                        <Label fx:id="generatingBalanceLabel" alignment="TOP_RIGHT" contentDisplay="RIGHT" layoutX="222.0" prefWidth="169.0" text="0.0 TAC" wrapText="true" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                    </children>
                                    <opaqueInsets>
                                        <Insets />
                                    </opaqueInsets>
                                </AnchorPane>
                            </children>
                            <padding>
                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                            </padding>
                        </VBox>
                        <VBox minWidth="400.0">
                            <padding>
                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                            </padding>
                            <children>
                                <HBox alignment="CENTER">
                                    <children>
                                        <Label styleClass="walletView-label" text="%leasing" />
                                    </children>
                                    <padding>
                                        <Insets bottom="10.0" />
                                    </padding>
                                </HBox>
                                <GridPane hgap="20.0" minHeight="100.0">
                                    <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" percentWidth="0.0" />
                                        <ColumnConstraints hgrow="SOMETIMES" />
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                    </rowConstraints>
                                    <children>
                                        <Label text="%recipient" />
                                        <TextField fx:id="recipientTextField" GridPane.columnIndex="1">
                                            <GridPane.margin>
                                                <Insets />
                                            </GridPane.margin>
                                        </TextField>
                                        <Label text="%amount" GridPane.rowIndex="1" />
                                        <TextField fx:id="amountTextField" GridPane.columnIndex="1" GridPane.rowIndex="1">
                                            <GridPane.margin>
                                                <Insets />
                                            </GridPane.margin>
                                        </TextField>
                                        <Label text="%fee" GridPane.rowIndex="2" />
                                        <Label fx:id="feeLabel" GridPane.columnIndex="1" GridPane.rowIndex="2">
                                            <GridPane.margin>
                                                <Insets left="3.0" />
                                            </GridPane.margin>
                                        </Label>
                                    </children>
                                </GridPane>
                                <HBox alignment="CENTER">
                                    <children>
                                        <Button fx:id="startLeasingButton" disable="true" mnemonicParsing="false" text="%start_leasing">
                                            <HBox.margin>
                                                <Insets />
                                            </HBox.margin>
                                        </Button>
                                    </children>
                                    <VBox.margin>
                                        <Insets />
                                    </VBox.margin>
                                    <padding>
                                        <Insets top="10.0" />
                                    </padding>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </HBox>
                <VBox>
                    <children>
                        <TableView fx:id="transactionTableView" prefHeight="262.0" prefWidth="480.0">
                            <placeholder>
                                <Label text="" />
                            </placeholder>
                            <columns>
                                <TableColumn fx:id="transactionTypeTableColumn" editable="false" prefWidth="390.0" text="%active_leases">
                                    <cellValueFactory>
                                        <PropertyValueFactory property="transactionSummary" />
                                    </cellValueFactory>
                                </TableColumn>
                                <TableColumn fx:id="transactionDateTableColumn" prefWidth="50.0" text="%date">
                                    <cellValueFactory>
                                        <PropertyValueFactory property="dateTime" />
                                    </cellValueFactory>
                                </TableColumn>
                            </columns>
                            <padding>
                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                            </padding>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                            </columnResizePolicy>
                        </TableView>
                    </children>
                </VBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
